{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string"
    },
    "picture": {
      "type": "text"
    },
    "email": {
      "type": "email"
    },
    "role": {
      "type": "enumeration",
      "enum": [
        "ADMIN",
        "MODERATOR",
        "SUBSCRIBER",
        "DEFAULT"
      ],
      "default": "DEFAULT"
    },
    "idFirebase": {
      "type": "text"
    },
    "comebacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comeback.comeback",
      "mappedBy": "customer"
    }
  }
}
