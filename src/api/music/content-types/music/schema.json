{
  "kind": "collectionType",
  "collectionName": "musics",
  "info": {
    "singularName": "music",
    "pluralName": "musics",
    "displayName": "Music"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "VideoID": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Duration": {
      "type": "integer"
    },
    "releases": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::release.release",
      "inversedBy": "music"
    },
    "artists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::artist.artist",
      "inversedBy": "musics"
    }
  }
}
