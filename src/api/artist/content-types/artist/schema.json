{
  "kind": "collectionType",
  "collectionName": "artists",
  "info": {
    "singularName": "artist",
    "pluralName": "artists",
    "displayName": "Artist",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "idYoutubeMusic": {
      "type": "string",
      "unique": true,
      "required": false
    },
    "releases": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::release.release",
      "mappedBy": "artists"
    },
    "description": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "SOLO",
        "GROUP"
      ],
      "default": "SOLO"
    },
    "socials": {
      "type": "json"
    },
    "styles": {
      "type": "enumeration",
      "enum": [
        "K-POP",
        "K-ROCK",
        "K-RAP",
        "K-RNB",
        "K-HIPHOP",
        "J-POP",
        "J-ROCK",
        "J-RAP",
        "J-RNB",
        "J-HIPHOP",
        "C-POP",
        "C-ROCK",
        "C-RAP",
        "C-RNB",
        "C-HIPHOP"
      ]
    },
    "platforms": {
      "type": "json"
    },
    "verified": {
      "type": "boolean",
      "default": false
    },
    "members": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::artist.artist",
      "inversedBy": "groups"
    },
    "groups": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::artist.artist",
      "mappedBy": "members"
    },
    "comebacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comeback.comeback",
      "mappedBy": "artist"
    },
    "musics": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::music.music",
      "mappedBy": "artists"
    },
    "images": {
      "type": "json"
    }
  }
}
