{
  "kind": "collectionType",
  "collectionName": "releases",
  "info": {
    "singularName": "release",
    "pluralName": "releases",
    "displayName": "Release",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "idYoutubeMusic": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "artists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::artist.artist",
      "inversedBy": "releases"
    },
    "dateRelease": {
      "type": "date",
      "required": true
    },
    "image": {
      "type": "string",
      "required": true
    },
    "year": {
      "type": "integer"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "ALBUM",
        "EP",
        "SINGLE"
      ],
      "default": "SINGLE"
    },
    "verified": {
      "type": "boolean",
      "default": true
    },
    "platforms": {
      "type": "json"
    },
    "music": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::music.music",
      "mappedBy": "releases"
    }
  }
}
